{
    "name": "ostheneo/nova-fields",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "OsTheNeo\\NovaFields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OsTheNeo\\NovaFields\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
